{
  "name": "react-visualizer",
  "version": "5.0.1",
  "description": "The visualizer in a react component",
  "main": "dist/src/index.js",
  "bin": {
    "makeVisualizerPage": "./dist/bin/makeVisualizerPage.js"
  },
  "scripts": {
    "build": "esbuild 'src/*' 'bin/*' --outdir=dist",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "watch-test": "esbuild test/app.js --bundle --outfile=test/built.js --watch",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cheminfo/visualizer-react.git"
  },
  "keywords": [
    "react",
    "visualizer"
  ],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/visualizer-react/issues"
  },
  "homepage": "https://github.com/cheminfo/visualizer-react#readme",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "@types/react": "^18.3.28",
    "esbuild": "^0.27.3",
    "prettier": "^3.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "files": [
    "dist"
  ],
  "volta": {
    "node": "24.14.0"
  }
}
